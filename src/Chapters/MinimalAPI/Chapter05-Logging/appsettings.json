{
  "Logging": {
    "LogLevel": { // Default provider
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "CategoryFiltered": "Information", // our custom CategoryFiltered record for Logging
      "Chapter05_Logging.Alerts.MyCategoryAlert": "Debug" // Out custom record MyCategoryAlert for Logging
    },
    "Console": { // Console provider.
      "LogLevel": {
        "Microsoft": "Information"
      }
    }
  },
  "AllowedHosts": "*"
}
